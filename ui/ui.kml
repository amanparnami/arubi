		<ScreenOverlay>
			<name>Overlay HTML</name>
			<description><![CDATA[
        
				<script type="text/javascript"> 

					var script = document.createElement('script'); 
					script.src = 'js/loader.js'; 
					script.type = 'text/javascript'; 
					script.onload = function(){ar_init();};
					document.body.appendChild(script);  
                
